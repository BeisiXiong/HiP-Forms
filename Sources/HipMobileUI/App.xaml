<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HipMobileUI.App"
             xmlns:converters="clr-namespace:HipMobileUI.Converters;assembly=HipMobileUI">
	<Application.Resources>
        <ResourceDictionary>
            <Color x:Key="PrimaryColor">#3F51B5</Color>
            <Color x:Key="PrimaryDarkColor">#303F9F</Color>
            <Color x:Key="AccentColor">#FF9800</Color>
            <Style TargetType="Label">
                <Setter Property="FontSize" Value="12" />
            </Style>
            <converters:ViewModelViewConverter x:Key="ViewToViewModelConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>